                    }

                    $('.manEkle-hata').attr('disabled', false);
                    if ($('.taksitMeblag-hata').val().length >= 3) {
                        $(this).css("border-color", "rgb(32, 191, 44)");
                    } else {
                        $(this).css("border-color", "rgb(247, 51, 51)");
                        $('.manEkle-hata').attr('disabled', true);
                    }

                    $('.otoEkle-hata').attr('disabled', false);
                    if ($('.taksitMeblag-hata').val().length >= 3) {
                        $(this).css("border-color", "rgb(32, 191, 44)");
                    } else {
                        $(this).css("border-color", "rgb(247, 51, 51)");
