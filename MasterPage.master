                    }

                    if ($(".textPwdNew").val() == '') {
                        $(".textPwdNew").css("border-color", "rgb(247, 51, 51)");
                        $('.btnPwdDegis').attr('disabled', true);
                    }

                    if ($(".textPwdNew2").val() == '') {
                        $(".textPwdNew2").css("border-color", "rgb(247, 51, 51)");
                        $('.btnPwdDegis').attr('disabled', true);
                    }



                });



