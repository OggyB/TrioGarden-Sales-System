                });

                $('.otoEkle-hata').click(function () {
                    if ($(".taksitMeblag-hata").val() == '0') {
                        $(".taksitMeblag-hata").css("border-color", "rgb(247, 51, 51)");
                        $('.otoEkle-hata').attr('disabled', true);
