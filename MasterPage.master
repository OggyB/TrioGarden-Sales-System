                        $(this).css("border-color", "rgb(247, 51, 51)");
                        $('.btnPwdDegis').attr('disabled', true);
                    }
                });

                $('#errorPwdNew').css('display', 'none');

                $('.textPwdNew').keyup(function () {
                    if ($(this).val() == '') {
                        $(this).css("border-color", "rgb(247, 51, 51)");
                        $('.btnPwdDegis').attr('disabled', true);
                        e.preventDefault();
                    } else {
                        $(this).css("border-color", "rgb(32, 191, 44)");
                    }

                    $('.btnPwdDegis').attr('disabled', false);
                    if ($('.textPwdNew').val().length >= 6) {
                        $(this).css("border-color", "rgb(32, 191, 44)");
                        $('#errorPwdNew').css('display', 'none');
                    } else {
                        $(this).css("border-color", "rgb(247, 51, 51)");
                        $('.btnPwdDegis').attr('disabled', true);
                        $('#errorPwdNew').css('display', 'block');
