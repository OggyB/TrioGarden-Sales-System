                $('.taksitMeblag-hata').keyup(function () {
                    if ($(this).val() == '') {
                        $(this).css("border-color", "rgb(247, 51, 51)");
                        $('.manEkle-hata').attr('disabled', true);
                        $('.otoEkle-hata').attr('disabled', true);
                        e.preventDefault();
                    } else {
